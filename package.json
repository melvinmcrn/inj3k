{
  "name": "inj3k-root",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/melvinmcrn/inj3k"
  },
  "scripts": {
    "build": "pnpm run --r --parallel build",
    "changeset:version": "changeset version && pnpm install --lockfile-only",
    "changeset:publish": "pnpm build && changeset publish",
    "preinstall": "npx only-allow pnpm",
    "lint": "pnpm prettier . --write --config .prettierrc",
    "lint:check": "pnpm prettier . --check --config .prettierrc",
    "format": "pnpm eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:typecheck": "vitest run --typecheck",
    "typecheck": "tsc --noEmit"
  },
  "license": "ISC",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@eslint/compat": "^1.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.6.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "eslint": "^9.6.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "globals": "^15.7.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@9.4.0"
}